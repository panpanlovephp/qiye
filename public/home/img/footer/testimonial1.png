            <script>
            $(document).ready( function() {
                $(".sslstore li a").click(function(event){
                    if(window.location.href=="http://cn.hostease.com/ssltest.php"){
                        event.preventDefault();
                        $("html,body").animate({scrollTop:$("#plans").offset().top},1000)
                    }
                    var sessionLink = $(this).attr("attr-link");
                    localStorage.setItem("attrLink",sessionLink);
                    var attrs = localStorage.getItem("attrLink");
                    $(".resp-tab-content").attr("aria-labelledby",function(index,old){
                            if(old == attrs){
                                $(".resp-tab-content").removeClass("resp-tab-content-active");
                                $(".resp-tab-content").css('display','none');
                                $(this).addClass("resp-tab-content-active");
                                $(this).css('display','block');
                                
                            }
                        }); 
                        $(".resp-tabs-list li").attr("aria-controls",function(index,old){
                            if(old == attrs){
                                $('.resp-tabs-list li').css({'background-color':'rgb(237, 237, 238)'});
                                $('.resp-tabs-list li').removeClass('resp-tab-active');
                                $(this).addClass('resp-tab-active');
                                $(this).css({'background-color':'#fff'});           
                            }
                        }); 
                })
            })
